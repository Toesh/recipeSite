{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "../out-tsc/app",
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["assets/*"],
			"@styles/*": ["app/styles/*"],
			"@components/*": ["app/components/*"],
			"@interfaces/*": ["app/interfaces/*"],
			"@models/*": ["styles/models/*"],
			"@pages/*": ["app/pages/*"],
			"@reducers/*": ["app/reducers/*"],
			"@services/*": ["app/services/*"]
		},
		"types": []
	},
	"files": ["main.ts", "polyfills.ts"],
	"include": ["src/**/*.d.ts"]
}
