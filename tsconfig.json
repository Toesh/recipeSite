{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": ".src",
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"incremental": true,
		"lib": [
			"es2017",
			"dom"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"outDir": "./dist/out-tsc",
		"paths": {
			"@rest/*": [
				"app/REST/*"
			],
			"@assets/*": [
				"assets/*"
			]
		},
		"sourceMap": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		]
	}
}
